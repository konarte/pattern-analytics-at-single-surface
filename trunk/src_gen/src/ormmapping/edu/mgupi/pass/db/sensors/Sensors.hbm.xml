<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="edu.mgupi.pass.db.sensors.Sensors" table="Sensors" lazy="false">
		<id name="idSensor" column="IdSensor" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="sensorType" column="SensorTypesIdSensorType" class="edu.mgupi.pass.db.sensors.SensorTypes" cascade="save-update,lock" unique="true" not-null="true" lazy="no-proxy">
		</many-to-one>
		<one-to-one name="mpathMaterial" class="edu.mgupi.pass.db.surfaces.Materials" cascade="save-update,lock" property-ref="sensor" lazy="no-proxy"/>
	</class>
</hibernate-mapping>
